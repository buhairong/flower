<!--components/movie-list/index.wxml-->

<view class="container f-class">
  <view class="title-container">
    <text>{{title}}</text>
    <text class="more-text">更多 ></text>
  </view>
  <view class="movie-container">
    <movie wx:for="{{movies}}" wx:key="id" movie="{{item}}"></movie>
  </view>
</view>
